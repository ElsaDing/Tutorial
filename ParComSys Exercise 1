      "cell_type": "markdown",
      "source": [
        "# **3. Calculate Speedup**"
      ],
      "metadata": {
        "id": "cf3c8ycJ6--8"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "\n",
        "# Speedup Measurement Function\n",
        "def measure_speedup(string, target_char, max_processors):\n",
        "    sequential_times = []\n",
        "    parallel_times = {\n",
        "        'EREW': [],\n",
        "        'CREW': [],\n",
        "        'CRCW': []\n",
        "    }\n",
        "    processors = range(1, max_processors + 1)  # Processors range from 1 to max_processors\n",
        "\n",
        "    # Measure sequential runtime\n",
        "    start = time.time()\n",
        "    sequential_search(string, target_char)\n",
        "    sequential_runtime = time.time() - start\n",
        "    sequential_times.append(sequential_runtime)\n",
        "\n",
        "    # Measure parallel runtimes\n",
        "    for p in processors:\n",
        "        # EREW-PRAM Runtime\n",
        "        start = time.time()\n",
        "        erew_pram_search(string, p)\n",
        "        parallel_times['EREW'].append(time.time() - start)\n",
        "\n",
        "        # CREW-PRAM Runtime\n",
        "        start = time.time()\n",
        "        crew_pram_search(string, p)\n",
        "        parallel_times['CREW'].append(time.time() - start)\n",
        "\n",
        "        # CRCW-PRAM Runtime\n",
        "        start = time.time()\n",
        "        crcw_pram_search(string, p)\n",
        "        parallel_times['CRCW'].append(time.time() - start)\n",
        "\n",
        "    # Calculate speedups\n",
        "    speedups = {\n",
        "        'EREW': [sequential_runtime / t for t in parallel_times['EREW']],\n",
        "        'CREW': [sequential_runtime / t for t in parallel_times['CREW']],\n",
        "        'CRCW': [sequential_runtime / t for t in parallel_times['CRCW']]\n",
        "    }\n",
        "\n",
        "    return processors, speedups\n",
        "\n",
        "# Plotting speedup\n",
        "def plot_speedup(processors, speedups):\n",
        "    \"\"\"\n",
        "    Function to plot speedup curves for the algorithms.\n",
        "    Args:\n",
        "        processors: Number of processors used for parallel algorithms.\n",
        "        speedups: Dictionary of speedup values for EREW, CREW, and CRCW.\n",
        "    \"\"\"\n",
        "    plt.figure(figsize=(10, 6))\n",
        "    for model, values in speedups.items():\n",
        "        plt.plot(processors, values, marker='o', label=model, linewidth=2)\n",
        "    plt.axhline(1, color='gray', linestyle='--', label='Sequential (baseline)')\n",
        "    plt.title('Speedup of Parallel Algorithms (EREW, CREW, CRCW)', fontsize=16)\n",
        "    plt.xlabel('Number of Processors (p)', fontsize=14)\n",
        "    plt.ylabel('Speedup (Sequential Time / Parallel Time)', fontsize=14)\n",
        "    plt.xticks(fontsize=12)\n",
        "    plt.yticks(fontsize=12)\n",
        "    plt.legend(fontsize=12)\n",
        "    plt.grid(True)\n",
        "    plt.tight_layout()\n",
        "    plt.show()\n",
        "\n"
      ],
      "metadata": {
        "id": "YSY5L50Lasly"
      },
      "execution_count": 18,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [],
      "metadata": {
        "id": "333s9B3sfZpB"
      },
      "execution_count": 12,
