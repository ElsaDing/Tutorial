        "# Run the tests and plot results\n",
        "if __name__ == \"__main__\":\n",
        "\n",
        "    max_processors = 16  # Maximum number of processors to test\n",
        "\n",
        "    processors, speedups = measure_speedup(string, target_char, max_processors)\n",
        "    plot_speedup(processors, speedups)"
